Object subclass: #Camion	instanceVariableNames: 'patente estado cargaMaxima cargas'	classVariableNames: ''	package: 'repasoParcial1'!!Camion methodsFor: 'initialization' stamp: 'EmilioGalimberti 4/28/2022 12:19'!initialize 	super initialize.	patente := ''.	estado := 'disponible'.	cargaMaxima := 1000.	cargas := OrderedCollection new. "nos permite que la coleccion se carge de forma ordenada no PONER NIL"! !!Camion methodsFor: 'control' stamp: 'EmilioGalimberti 4/28/2022 13:02'!bajarrCarga: unaCarga	|ok|	ok := false.                      "â†“ se valida si la carga existe"	(unaCarga isKindOf: Carga) ifTrue: [ 		((estado = 'disponible') & (cargas includes: unaCarga )) ifTrue: [ 			cargas remove: unaCarga  	].	ok := true	].^ok! !!Camion methodsFor: 'control' stamp: 'EmilioGalimberti 4/28/2022 13:03'!subirCarga: unaCarga	|ok|	ok := false.	(unaCarga isKindOf: Carga) ifTrue: [ 		((estado = 'disponible') & (self peso + unaCarga peso <= cargaMaxima)) ifTrue: [ 			cargas add: unaCarga  	].		ok := true.	].^ok.! !!Camion methodsFor: 'control' stamp: 'EmilioGalimberti 4/28/2022 12:36'!peso	|total|	total := 0.	cargas do:[:x| total := total + x peso].	^total  ! !!Camion methodsFor: 'control' stamp: 'EmilioGalimberti 4/28/2022 13:11'!cargasEnOrden	|cad|	cad := 'Listado de Cargas en Orden de peso: ', String cr.	cargas asSortedCollection.	cargas do:[:x| cad := cad, x asString, String cr.].	^cad	! !!Camion methodsFor: 'accessing' stamp: 'EmilioGalimberti 4/28/2022 12:03'!estado	^estado.! !!Camion methodsFor: 'accessing' stamp: 'EmilioGalimberti 4/28/2022 12:04'!estado: anObject	estado := anObject ! !!Camion methodsFor: 'accessing' stamp: 'EmilioGalimberti 4/28/2022 12:08'!patente: anObject patente := anObject 	! !!Camion methodsFor: 'accessing' stamp: 'EmilioGalimberti 4/28/2022 12:20'!cargaMaxima	^ cargaMaxima! !!Camion methodsFor: 'accessing' stamp: 'EmilioGalimberti 4/28/2022 12:05'!patente	^patente! !!Camion 